<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.app.hamzaworld">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

    <application
        android:name=".other.HamzaWorld"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".activity.TrackActivity"
            android:screenOrientation="portrait"
            android:parentActivityName=".activity.MoreActivity"/>
        <activity
            android:name=".activity.BagDetailActivity"
            android:parentActivityName=".activity.BagActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.OrderDetailActivity"
            android:parentActivityName=".activity.OrderActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.RateActivity"
            android:parentActivityName=".activity.MoreActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.PrivacyActivity"
            android:parentActivityName=".activity.MoreActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.SupportActivity"
            android:parentActivityName=".activity.MoreActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.BagActivity"
            android:parentActivityName=".activity.MoreActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.OrderActivity"
            android:parentActivityName=".activity.MoreActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.MoreActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.PlaceActivity"
            android:screenOrientation="portrait"
            android:theme="@style/TransparentTheme" />
        <activity
            android:name=".activity.CheckoutActivity"
            android:parentActivityName=".activity.CartActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.ProfileActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name=".activity.CartActivity"
            android:parentActivityName=".activity.HomeActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.GrabProductActivity"
            android:parentActivityName=".activity.HomeActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.GrabOfferActivity"
            android:parentActivityName=".activity.HomeActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.DetailActivity"
            android:parentActivityName=".activity.ProductActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.ProductActivity"
            android:parentActivityName=".activity.BrandActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.BrandActivity"
            android:parentActivityName=".activity.CategoryActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.CategoryActivity"
            android:parentActivityName=".activity.HomeActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.HomeActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>